{
  "kind": "collectionType",
  "collectionName": "tokens",
  "info": {
    "singularName": "token",
    "pluralName": "tokens",
    "displayName": "Token",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "tokenId": {
      "type": "integer"
    },
    "displayName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "tokenAddress": {
      "type": "string",
      "regex": "^0x[a-fA-F0-9]{40}$"
    },
    "contractAddress": {
      "type": "string",
      "regex": "^0x[a-fA-F0-9]{40}$"
    },
    "attributes": {
      "type": "component",
      "repeatable": true,
      "component": "token.attribute"
    },
    "package": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::package.package"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Draft",
        "Generating",
        "Published",
        "Archived"
      ],
      "default": "Draft"
    }
  }
}
